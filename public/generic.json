function Room(id, rules, time_limit, date_created, creator) {
	var roomid = {
		id: id,
		rules: rules,
		time_limit: time_limit,
		date_created: date_created,
		completion_status: 0,
		users: {}
	};
	roomid.users[creator] = newUser(creator);
	return roomid;
}

function newUser(userName) {
	var userName = {
		id: userName
		recording: null
	};
	return userName;
}

